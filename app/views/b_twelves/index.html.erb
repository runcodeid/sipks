<p id="notice"><%= notice %></p>

<h1>B Twelves</h1>

<table>
  <thead>
    <tr>
      <th>Action plan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @b_twelves.each do |b_twelf| %>
      <tr>
        <td><%= b_twelf.action_plan %></td>
        <td><%= link_to 'Show', b_twelf %></td>
        <td><%= link_to 'Edit', edit_b_twelf_path(b_twelf) %></td>
        <td><%= link_to 'Destroy', b_twelf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= form_with(model: b_four, local: true) do |form| %>
  <% if b_four.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(b_four.errors.count, "error") %> prohibited this b_four from being saved:</h2>

      <ul>
      <% b_four.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :action_plan_id %>
    <%= form.text_field :action_plan_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>



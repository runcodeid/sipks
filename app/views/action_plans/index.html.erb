<p id="notice"><%= notice %></p>

<h1>Action Plans</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Year</th>
      <th>Code action plans</th>
      <th>Action plan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @action_plans.each do |action_plan| %>
      <tr>
        <td><%= action_plan.city %></td>
        <td><%= action_plan.year %></td>
        <td><%= action_plan.code_action_plans %></td>
        <td><%= action_plan.action_plan %></td>
        <td><%= link_to 'Show', action_plan %></td>
        <td><%= link_to 'Edit', edit_action_plan_path(action_plan) %></td>
        <td><%= link_to 'Destroy', action_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Action Plan', new_year_action_plan_path %>
